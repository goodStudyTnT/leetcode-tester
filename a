    <script>
      var navbar = document.querySelector('#noj-navbar')
      var placeholder = document.querySelector('#noj-navbar-placeholder')
      var noNeedExtraPaddingPaths = ['/articles', '/contest', '/explore', '/store', '/playground', '/points']
      var noNeedExtraPadding = noNeedExtraPaddingPaths.some(function(path) {
        return location.pathname.startsWith(path)
      })

      function isMobile() {
        return !!navigator.userAgent.match(/Android|iPod|iPhone/i);
      }

      // add .pc-body to control min-width on pc
      if(!isMobile()){
        document.body.className += ' pc-body';
      }

      if (noNeedExtraPadding) {
        placeholder.style.height = '44px'
      }
      var observer = new MutationObserver(function(list, obs) {
        placeholder.parentNode.removeChild(placeholder)
        if (isMobile()) {
          if (!noNeedExtraPadding) {
            navbar.style.marginBottom = '65px'
          } else {
            navbar.style.marginBottom = '45px'
          }
          navbar.style.position = 'relative'
        } else {
          if (!noNeedExtraPadding) {
            navbar.style.marginBottom = '20px'
          }
        }
        obs.disconnect()
      })

      observer.observe(navbar, {
        childList: true
      })
    </script>
