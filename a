            tbody.reactable-data tr > td:nth-child(6) {
                order: -1;
                margin-left: -3px;
            }
            .question-list-table span.level-hard {
                color: #fe3e2f;
            }
            .question-list-table span.level-medium {
                color: #fe9500;
            }
            .question-list-table span.level-easy {
                color: #35c759;
            }
            span[class*="level-"] {
                font-size: 12px;
                border-radius: 5px;
                background: ${qDiffBG};
                padding: 2px 5px;
                white-space: nowrap;
            }
            span[class*="level-"]::after {
                
            }
        `;
        var appOnlyHTML = document.createElement("div");
        appOnlyHTML.innerHTML = `
            <style>
            ${appOnlyStyle}
            ${applyDarkstyle}
            </style>
        `;
        var appInsert = document.querySelector(".content-wrapper");
        appInsert.insertBefore(appOnlyHTML, document.getElementById("base_content"));
    }
}
</script>









