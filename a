<!--@start lc-vscode 竞赛合作 -->
<script>

if (/contest\/$/.test(location.href)) {
    var cnt = 0
    function changeLcvscodeNode() {
      if (cnt > 3) {
        return
      }
      cnt++
      var element
      for (const node of document.querySelectorAll('.title')) {
        if (node.innerText === '「微爱思扣 以 Code 会友」专场竞赛') {
          element = node
        }
      }
      if(element) {
        element.style.maxWidth = '130px'
      } else {
        setTimeout(()=>{
          changeLcvscodeNode()
        }, 100)
      }
    }
    changeLcvscodeNode()
  }

  if (/contest\/lc-vscode\/ranking/.test(location.href)) {
    if (new Date() < new Date('2021/06/19 16:40 +0800')) {
      __renderLcVscodeAd()
    }
  }

  function __renderLcVscodeAd() {
    var container = document.createElement('div')
    container.innerHTML = `
      <a href="https://datayi.cn/w/WoM3eLBR" target="_blank">
      <img src='https://assets.leetcode-cn.com/contest/lc-vscode/lc-vscode.png' width="100%" />
      </a>      
`
    function render() {
      var parent = document.querySelector('.contest-base .container')
      if (!parent) {
        setTimeout(()=>{
          render()
        }, 100)
      } else{
        parent.appendChild(container)
      }
    }
    render()
  }
</script>
<!--@end lc-vscode 竞赛合作 -->
    

    
